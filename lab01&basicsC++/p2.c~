#include <stdio.h>

int main()
{
  int x = 12;  // set to different values to test your program
  int j = 1;
  int count = 0;
  while (j < x) {
    if ((x % j) == 0) {
      ++count;
      ++j;
    }
    else {
      ++j;  
    }
  }

  printf("%d can be divided by %d number(s)\n", x, count);
  return 0;
}
